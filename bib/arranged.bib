  author = {Cerqueira-Silva, Thiago, Oliveira, Vinicius de Araújo, Boaventura, Viviane S., Pescarini, Julia M., Júnior, Juracy Bertoldo, Machado, Tales Mota, Flores-Ortiz, Renzo, Penna, Gerson O., Ichihara, Maria Yury, de, Barros, Jacson Venâncio, Barreto, Mauricio L., Werneck, Guilherme Loureiro, Barral-Netto, Manoel},
  container-title = {The Lancet Regional Health - Americas},
  id = {cerqueira-silva_influence_2022},
